{
  "name": "obsidian-amazingmarvin-plugin",
  "version": "0.0.1",
  "description": "This is a plugin for Obsidian (https://obsidian.md) for Amazing Marvin (https://app.amazingmarvin.com/)",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "main": "src/main.ts",
  "scripts": {
    "build": "rollup --config rollup.config.js --environment BUILD:production",
    "dev": "rollup --config rollup.config.js -w",
    "dev-install": "npm run build && node ./scripts/dev-install.js",
    "format": "prettier --write src/**/*",
    "lint": "prettier --check src/**/*",
    "dev:install:watch": "concurrently -k \"yarn dev\" \"chokidar 'main.js' -c 'npm run dev-install'\"",
    "pack-release": "npm run build && node ./scripts/pack-release.js"
  },
  "resolutions": {
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@codemirror/state": "6.5.0",
    "@codemirror/view": "6.38.6",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "obsidian": "^1.10.3",
    "obsidian-daily-notes-interface": "^0.9.2",
    "postcss": "^8.4.24",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.52.7",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.38.3",
    "svelte-awesome": "^2.3.2",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.7.3",
    "archiver": "^5.3.0",
    "tslib": "^2.3.0",
    "typescript": "^4.9.5"
  }
}
